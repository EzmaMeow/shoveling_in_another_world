[sub_resource type="GDScript" id="GDScript_3m0nd"]
script/source = "extends CharacterBody2D
#this is a test for something unrelated. 
#but could be salvage when designing ai follow target
#though i tend to hand ai diffrently. 
@export var player : Node2D

var speed : float = 160

func _physics_process(delta):
	var min_distance : float = 160.0
	var max_distance : float = 220.0
	var distance : float = global_position.distance_to(player.global_position)
	var current_speed : float = lerpf(0.0, speed, (distance-min_distance)/max_distance)
	if distance > min_distance || distance < min_distance :
		velocity = (player.global_position - global_position).normalized() * current_speed
	else:
		velocity = Vector2.ZERO
	move_and_slide()
"

